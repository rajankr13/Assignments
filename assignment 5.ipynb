   "source": [
    "# Write a Python class to implement pow(x, n)\n",
    "class power:\n",
    "    def __init__(self,x,n):\n",
    "        self.x=x\n",
    "        self.n=n\n",
    "        self.result=0\n",
    "        \n",
    "    def nth_power(self):\n",
    "        self.result=pow(self.x,self.n)\n",
    "        print(self.result)\n",
    "\n",
    "find=power(10,2)\n",
    "find.nth_power()"
   ]
